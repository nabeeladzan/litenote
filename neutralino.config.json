{
	"applicationId": "js.neutralino.zero",
	"version": "1.0.0",
	"defaultMode": "window",
	"documentRoot": "/build/",
	"url": "/",
	"enableServer": true,
	"enableNativeAPI": true,
	"nativeAllowList": ["app.*", "filesystem.*", "computer.*", "os.*"],
	"modes": {
		"window": {
			"title": "litenote",
			"width": 800,
			"height": 500,
			"minWidth": 400,
			"minHeight": 200,
			"icon": "/public/logo192.png"
		}
	},
	"cli": {
		"binaryName": "litenote",
		"resourcesPath": "/build",
		"extensionsPath": "/extensions/",
		"clientLibrary": "/public/neutralino.js",
		"binaryVersion": "4.9.0",
		"clientVersion": "3.8.0",
		"frontendLibrary": {
			"patchFile": "/public/index.html",
			"devUrl": "http://localhost:3000"
		}
	}
}
